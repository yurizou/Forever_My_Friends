<h2>スケジュール一覧</h2>

<%= link_to "+", new_plan_path %>

<div>
    <% @plans.each do |plan| %>
    <%= link_to plan_path(plan) do %>
    <div>
        <div>
            <%= plan.plan_date.strftime("%Y年%m月%d日") %>
        </div>
        <%= plan.title %>
        <%= plan.status %>
    </div>
    <% end %>
  <% end %>
</div>