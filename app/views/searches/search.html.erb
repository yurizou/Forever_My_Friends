<div class="container">
  <div class="row my-4">
    <h2 class="col-lg-3 mb-5 offset-3">検索結果一覧</h2>
  </div>
  
  <div class="row my-3">
    <div class="col-lg-5 offset-3">
      <table class="table table-hover table-inverse">
  <!--検索対象モデルがUserの時 -->
        <tbody>
          <% @friends.each do |friend| %>
            <tr>
              <%# binding.pry %>
             　<%#= link_to friend_path(friend) do %>
                <td><%= link_to friend.nickname, friend_path(friend.id) %></td>
              <%#end %>
            </tr>
          <% end %>
          
          <% @plans.each do |plan| %>
          <tr>
            <td><%= link_to plan.title, plan_path(plan.id) %></td>
          </tr>
          <% end %>
        </tbody>
    </table>
    </div>
  </div>
</div>




